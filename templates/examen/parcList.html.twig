<h2>Listes des parc</h2>
<table border="2">
    <tr>
        <th>ID</th>
        <th>Nom</th>
        <th>Adresse</th>
        <th>Actions</th>
    </tr>
    {% for i in parc %}
        <tr>
            <td>{{ i.id }}</td>
            <td>{{ i.nom}}</td>
            <td>{{ i.adresse }}</td>
            <td>
                <a href="{{ path('parcUpdate',{'id':i.id}) }}">Update</a>
                <a href="{{ path('parcRemove',{'id':i.id}) }}">Remove</a>
            </td>
        </tr>
    {% endfor %}
</table>
<a href="{{ path('parcAdd') }}">Ajouter un parc</a>
